systemLog:
    destination: file
    path: "/var/log/mongodb/mongod.log"
    logAppend: true
storage:
    journal:
        enabled: true
processManagement:
    fork: true
    pidFilePath: "/var/run/mongodb/mongod.pid"
net:
    bindIp: 0.0.0.0
    port: <%= @config_server_port %>
setParameter:
    enableLocalhostAuthBypass: false
storage:
    dbPath: "/var/lib/mongo"
sharding:
    clusterRole: configsvr
